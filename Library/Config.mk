# Config.mk - Global configuration
RUDIX=rudix-snowleopard
RUDIX_UNIVERSAL=yes
RUDIX_DISABLE_DEPENDENCY_TRACKING=yes
RUDIX_SAVE_CONFIGURE_CACHE=yes
RUDIX_STRIP_PACKAGE=yes
