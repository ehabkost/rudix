RUDIX_BUILD_ONLY_STATIC_LIB=no
RUDIX_PARALLEL_EXECUTION=yes
RUDIX_APPLY_RECOMMENDATIONS=yes
RUDIX_LABELS=Rudix-2012,OSX-SnowLeopard,XCode-4.2
